<div class="container" ng-controller="TutorialsController as app">
  <div class="row"></div> 
  <div class="container">
    <div class="row">
      <h4 class= "col s12">{{app.tutorial.name}}</h4>
    </div>
  </div>


  <div class="parallax-container" ng-repeat="lesson in app.lessons">
      
      <div class= "row">      
        <div class="col l6 m6 s12">
        <h6>{{lesson.name}}</h6>
        <p>{{lesson.body}}</p></div>
        <div class="col l6 m6 s12">
          <div class="responsive-img">
            <img class="responsive-img" src="http://obiaudio.com/wp-content/uploads/2010/07/frequency_chart_lg.gif">
          </div>
        </div>
        </div>

      <div class="row">
        <div class="col l6 m6 s12"></div>
        <div class="col l6 m6 s12"></div>
      </div>
       <div class="row">
        <div class="col l6 m6 s12"></div>
        <div class="col l6 m6 s12"></div>
      </div>

  </div>

<div class="fixed-action-btn" style="bottom: 45px; right: 24px;">
  <a class="btn-floating btn-large waves-effect waves-light red"><i class="mdi-content-add"></i></a>
</div>  
