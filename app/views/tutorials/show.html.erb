<div class="container" ng-controller="TutorialsController as app">
  <div class="container">
    <div class="row">
      <h3 class="bold">{{app.tutorial.name}}</h3>
    </div>
  </div>


  <div class="parallex" ng-repeat="lesson in app.lessons">
      
      <div class= "row">      
        <div class="col l6 m6 s12">
        <h6>{{lesson.name}}</h6>
        <p>{{lesson.body}}</p></div>
        <div class="col l6 m6 s12">
          <div class="circle responsive-img">
            <img src="http://obiaudio.com/wp-content/uploads/2010/07/frequency_chart_lg.gif" class="responsive-img"></div>
            </div>
          </div>

      <div class="row">
        <div class="col l6 m6 s12"></div>
        <div class="col l6 m6 s12"></div>
      </div>

  </div>

<div class="fixed-action-btn" style="bottom: 45px; right: 24px;">
  <a class="btn-floating btn-large waves-effect waves-light red"><i class="mdi-content-add"></i></a>
</div>  
